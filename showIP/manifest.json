{
  "manifest_version": 2,
  "name": "显示我的IP",
  "version": "1.0.0.0",
  "description": "chrome extension 显示我的IP",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "显示IP",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://api.ttt.sh/ip/qqwry/"
  ]
}